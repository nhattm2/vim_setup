{
  "suggest.noselect" : false,
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnSaveFiletypes": ["go", "dart","python"],
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "go"
      ],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  }
}
